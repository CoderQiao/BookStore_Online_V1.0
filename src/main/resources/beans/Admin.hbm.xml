<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="cn.qtlplay.bookstore.beans.Admin" table="admin" schema="bookstore">
        <id name="adminId">
            <column name="ADMIN_ID" sql-type="int(4)"/>
        </id>
        <property name="loginName">
            <column name="LOGIN_NAME" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="password">
            <column name="PASSWORD" sql-type="varchar(20)" length="20"/>
        </property>
    </class>
</hibernate-mapping>